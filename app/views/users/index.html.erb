<div class="callout large banner">
  <div class="grid-x grid-padding-x">
    <%= render 'layouts/alerts' if notice %>
    <h5 class="cell small-12 banner-items text-center">Mindly blogs</h5>
  </div>
</div>

<% @users.each do |user| %>
    <div class="blog-post">
        <%= render partial: "user",  locals: { user: user } %>
    </div>
<% end %>